{
  "name": "Musify - Music Downloader & Player",
  "short_name": "Musify",
  "description": "Download and stream music with embedded metadata, cover art, and smart recommendations. Your personal music player.",
  "author": "Lamgerr",
  "version": "2.0.0",
  "start_url": "/songs?source=pwa",
  "scope": "/",
  "display": "standalone",
  "display_override": ["standalone", "minimal-ui"],
  "background_color": "#0f0f23",
  "theme_color": "#1a1a2e",
  "orientation": "any",
  "lang": "en",
  "dir": "ltr",
  "id": "/songs",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "/static/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/static/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/static/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Musify Desktop Player"
    },
    {
      "src": "/static/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Musify Mobile Player"
    }
  ],
  "categories": ["music", "entertainment", "utilities"],
  "iarc_rating_id": "",
  "shortcuts": [
    {
      "name": "Download Music",
      "short_name": "Download",
      "description": "Search and download music from YouTube",
      "url": "/?source=shortcut",
      "icons": [
        {
          "src": "/static/icons/icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Music Library",
      "short_name": "Library",
      "description": "Browse your music library",
      "url": "/songs?source=shortcut",
      "icons": [
        {
          "src": "/static/icons/icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "song_name",
      "text": "song_name",
      "url": "song_name"
    }
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+musify",
      "url": "/songs?track=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/songs",
      "accept": {
        "audio/mpeg": [".mp3"],
        "audio/mp4": [".m4a"],
        "audio/ogg": [".ogg"],
        "audio/flac": [".flac"],
        "audio/wav": [".wav"]
      }
    }
  ],
  "related_applications": [],
  "serviceworker": {
    "src": "/static/sw.js",
    "scope": "/",
    "update_via_cache": "none"
  }
}
